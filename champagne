    emptyArtistText: "Untitled artist",
    emptyAlbumText: "",
    
    titleLabel: "Track:",
    artistLabel: "Artist:",
    albumLabel: ""
});
</script>

<script>
function myFunction() {
  var x = document.getElementById("myDIV");
  if (x.style.display === "block") {
    x.style.display = "none";
  } else {
    x.style.display = "block";
  }
}
</script>

